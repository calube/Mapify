# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'mapify' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for mapify

  # Branch
  pod 'Branch'

  # Facebook
  pod 'FacebookCore'
  pod 'FacebookLogin'

  # Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Firebase/Crash'
  pod 'Firebase/DynamicLinks'

  # SnapKit
  pod 'SnapKit', '~> 4.0.0'

  # SwiftyJSON
  pod 'SwiftyJSON'


  target 'mapifyTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
